{
  "info": {
    "name": "Tool Jockey API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "List tools",
      "request": {
        "method": "GET",
        "header": [
          { "key": "Authorization", "value": "Bearer dev-admin-token", "type": "text" }
        ],
        "url": {
          "raw": "{{baseUrl}}/tools",
          "host": ["{{baseUrl}}"],
          "path": ["tools"]
        }
      }
    }
  ]
}
